{
  "id": "shemetz-macros",
  "title": "Shemetz's Macros",
  "description": "shem's big pile o' macros, plus some basic module code to tie them together nicely",
  "version": "10.11.3",
  "compatibility": {
    "minimum": "10",
    "verified": "10"
  },
  "esmodules": [
    "scripts/shemetz-macros.js",
    "scripts/sockets.js",
    "scripts/hooks.js"
  ],
  "socket": true,
  "styles": [
    "styles/shemetz-macros.css"
  ],
  "packs": [
    {
      "name": "shemetz-macros",
      "label": "shemetz-macros",
      "module": "shemetz-macros",
      "path": "./packs/shemetz-macros.db",
      "type": "Macro"
    },
    {
      "name": "critical-hits",
      "label": "critical-hits",
      "module": "shemetz-macros",
      "path": "./packs/critical-hits.db",
      "type": "RollTable"
    }
  ],
  "relationships": {
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json",
        "compatibility": {
          "minimum": "1.12.7.0",
          "verified": "1.12.7.0"
        }
      }
    ]
  },
  "authors": [
    {
      "name": "shemetz",
      "discord": "shemetz#0226",
      "email": "itamarcu+foundry@gmail.com",
      "reddit": "u/Shemetz"
    }
  ],
  "url": "https://github.com/shemetz/shemetz-macros",
  "manifest": "https://github.com/shemetz/shemetz-macros/raw/master/module.json",
  "download": "https://github.com/shemetz/shemetz-macros/archive/master.zip",
  "readme": "https://github.com/shemetz/shemetz-macros/blob/master/Readme.md",
  "bugs": "https://github.com/shemetz/shemetz-macros/issues",
  "license": "https://github.com/shemetz/shemetz-macros/blob/master/License.txt"
}
